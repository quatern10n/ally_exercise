package com.example.restservice;

import java.util.Arrays;
import java.util.List;
import java.util.Random;

import org.springframework.stereotype.Component;

@Component
public class UnicornProvider {
	
	
	
	public UnicornProvider() {


	}
	
	public List<Unicorn> getAllUnicorns(){
		Unicorn unicorn1 = new  Unicorn("Excelsior", "brown", 1,"yellow", 8, "feet", 1000, "pounds");
        Unicorn unicorn2 = new Unicorn("Prissball", "Black", 2, "white", 6, "feet", 800, "pounds");
  
	    List<Unicorn> all = Arrays.asList(unicorn1, unicorn2);
	    return all;
	}
}
